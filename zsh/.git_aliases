# unadd a file
alias gunadd='git reset HEAD'

gitPushAll() {
	export tmp=$(git rev-parse --abbrev-ref HEAD)
	git push origin $1 && git push old-origin $1
	unset tmp
}
alias gpushall='gitPushAll'

gitPullFrom() {
	export tmp=$(git rev-parse --abbrev-ref HEAD)
	git stash
	git checkout $1
	git pull
	git checkout $tmp
	git stash pop
	unset tmp
}
alias gpullfrom='gitPullFrom'
alias gpulldev='gitPullFrom development'
alias gpullmaster='gitPullFrom master'

gitMergeFrom() {
	export tmp=$(git rev-parse --abbrev-ref HEAD)
	git stash
	git checkout $1
	git pull
	git checkout $tmp
	git merge $1
	git stash pop
	unset tmp
}
alias gmergefrom='gitMergeFrom'
alias gmergedev='gitMergeFrom development'

gitRenameBranch() {
	git branch -m $1 $2         		# Rename branch locally
	git push origin :$1                 # Delete the old branch
	git push --set-upstream origin $2   # Push the new branch, set local branch to track the new remote
}
alias grename='gitRenameBranch $1 $(git rev-parse --abbrev-ref HEAD)'
alias gbranchrename='gitRenameBranch $1 $(git rev-parse --abbrev-ref HEAD)'

alias ggraph="git log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --all"
alias ggraph2="git log --graph --all --format=format:'%C(bold yellow)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(bold white)- %an%C(reset)' --abbrev-commit"

